msgid ""
"We're sorry. Email wasn't sent to your email due to our email service is "
"down or busy."
msgstr ""
"Es tut uns leid. E-Mail nicht Ihre E-Mail durch unsere E-Mail-Dienst "
"gesendet wird, nach unten oder besetzt ist."

# application/modules/auth/views/reset_password_sent.php ( Line: 18 )
msgid "Sorry for any incovenience causes."
msgstr "Sorry für die Unannehmlichkeiten verursacht."

# application/modules/auth/views/reset_password_sent.php ( Line: 22 )
msgid "Please check your email"
msgstr "Bitte überprüfen Sie Ihre E-Mail-"

# application/modules/auth/views/reset_password_sent.php ( Line: 23 )
msgid "We've sent you email that will allow you to reset your password."
msgstr ""
"Wir haben Ihnen per E-Mail, mit dem Sie Ihr Passwort zurücksetzen gesendet."

# application/modules/auth/views/reset_password_sent.php ( Line: 30 )
msgid "Check your email now"
msgstr "Überprüfen Sie Ihre E-Mail jetzt"

# application/modules/auth/views/username_sent.php ( Line: 25 )
msgid "We've sent you email attached your username in it."
msgstr "Wir haben Ihnen angebracht Ihren Benutzername E-Mail in das gesendet."

